<script src='/js/jquery-1.7.min.js'></script>
<script>
    $(function () {
        $('#js-btn-signup').click(function (s) {
            s.preventDefault(), $.ajax({
                type: 'POST',
                url: '/newsletter/validate//',
                data: $('#js-form-lead-gen').serialize(),
                success: function (s) {
                    s = jQuery.parseJSON(s);
                    $("#submissionResult").html(s.message);
                    $('#js-form-lead-gen').hide();
                    $('#js-show-success-msg').show();
                }
            })
        });
    });

    $('ul.cms-tabs li').click(function () {
        var t = $(this).attr('data-tab');
        $('ul.cms-tabs li').removeClass('current');
        $('.cms-tab-content').removeClass('current');
        $(this).addClass('current');
        $('#' + t).addClass('current');
    });

    function getTimeRemaining(e) {
        var t = Date.parse(e) - Date.parse(new Date), a = Math.floor(t / 1e3 % 60), n = Math.floor(t / 1e3 / 60 % 60), s = Math.floor(t / 36e5 % 24), o = Math.floor(t / 864e5);
        return{total: t, days: o, hours: s, minutes: n, seconds: a}
    }
    function initializeClock(e) {
        function t() {
            var t = getTimeRemaining(e);
            $('#days').html('' + t.days), $('#hours').html(('0' + t.hours).slice(-2)), $('#minutes').html(('0' + t.minutes).slice(-2)), $('#seconds').html(('0' + t.seconds).slice(-2)), t.total <= 0 && clearInterval(a)
        }
        t();
        var a = setInterval(t, 1e3)
    }

    var deadline = 'Thu Nov 9 2017 00:00:00 GMT';
    initializeClock(deadline);

    $("#js-add-to-cal").click(function (event) {
        event.preventDefault();
        window.open("https://www.google.com/calendar/render?action=TEMPLATE&text=Spend%2011.11%20Singles'%20Day%20with%20ZALORA!&dates=20170911T161000Z/20161211T155900Z&details=Between%209th%20to%2012th%20November%2C%20shop%20to%20your%20heart%E2%80%99s%20content!%0A-%20Dig%20in%20to%20all%20our%20brand%20exclusive%20Single%27s%20Day%20deals!%0A-%20Complete%20all%20your%20festive%20shopping%20at%20one%20go!%0A%0AEnd%20this%20year%20off%20with%20a%20boost%20of%20retail%20therapy%2C%20you%20totally%20deserved%20it.%20Shop%20now!&location=ZALORA%20SG&sprop=&sprop=name:", "popupWindow", "width=600,height=600,scrollbars=yes");
    });
</script>
