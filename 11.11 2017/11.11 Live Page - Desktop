<script>
    function loadFallback() {
        if (!$('.dy_unit').is(':visible')) {

            /* Start Loading Fallback */
            $('.cms-fallback img').each(function () {
                this.src = $(this).data('src');
            });
            $('.cms-fallback').show();

            console.log('Loaded Fallback');
        } else {
            $('.cms-fallback').remove();
            console.log('Do Not Load Fallback');
        }
    }
    function detectEvergageStatus() {
        var timeoutID = window.setTimeout(loadFallback, 8000);
    }
    $(function () {
        detectEvergageStatus();
    });

    $('#js-scrollBtn-dailydeals').on('click', function () {
        $('html,body').animate({scrollTop: $('#js-scrollTo-dailydeals').offset().top}, 500);
        return false;
    });
    $('#js-scrollBtn-hourlydeals').on('click', function () {
        $('html,body').animate({scrollTop: $('#js-scrollTo-hourlydeals').offset().top}, 500);
        return false;
    });
    $('#js-scrollBtn-brands').on('click', function () {
        $('html,body').animate({scrollTop: $('#js-scrollTo-brands').offset().top}, 500);
        return false;
    });
    $('#js-scrollBtn-competition').on('click', function () {
        $('html,body').animate({scrollTop: $('#js-scrollTo-competition').offset().top}, 500);
        return false;
    });
    $('#js-scrollBtn-rewards').on('click', function () {
        $('html,body').animate({scrollTop: $('#js-scrollTo-rewards').offset().top}, 500);
        return false;
    });

    /* back to top button */
    $(window).scroll(function () {
        if ($(this).scrollTop() < 0)
            $('.cms-back-top').css('display', 'none');
        else
            $('.cms-back-top').css('display', 'block');
    });
    $('.cms-back-to-top').on('click', function () {
        $('html,body').animate({scrollTop: 0}, 500);
        return false;
    });
</script>
